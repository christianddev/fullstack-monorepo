{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "noEmitOnError": true,
  },
  "include": [
    "src",
    ".eslintrc.js",
    "package.json"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
}