{
  "name": "server",
  "version": "1.0.2",
  "private": true,
  "description": "Automated Express Backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn webpack",
    "start": "node dist/index.js",
    "mon": "nodemon -q dist/index.js",
    "watch": "yarn webpack watch",
    "dev": "yarn build && NODE_ENV=development yarn concurrently \"yarn watch\" \"yarn mon\"",
    "test": "yarn jest",
    "clean": "rm -rf node_modules dist"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-basic-auth": "^1.2.1",
    "express-jwt": "^7.7.5",
    "http-status": "^1.5.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "pg": "^8.7.3",
    "sequelize": "^6.19.1",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "umzug": "^3.2.1",
    "uuid": "^8.3.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
